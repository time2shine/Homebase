{
  "manifest_version": 2,
  "name": "Homebase",
  "version": "1.0",
  "description": "Replaces the default new tab page with a custom dashboard.",

  "icons": {
    "48": "icons/icon-48.png"
  },

  "applications": {
    "gecko": {
      "id": "rokonmagura@egmail.com"
    }
  },

  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  },

"permissions": [
    "tabs",
    "storage",
    "history",
    "bookmarks",
    "clipboardRead",
    "contextualIdentities",

    "https://api.quotable.io/*",
    "https://api.open-meteo.com/*",
    "https://geocoding-api.open-meteo.com/*",
 
    "https://duckduckgo.com/ac/*",
    "https://suggest.yandex.com/*",
    "https://ff.search.yahoo.com/*",
    "https://api.bing.com/osjson.aspx*",
    "https://suggestqueries.google.com/*",
     
    "https://en.wikipedia.org/*",
    "https://completion.amazon.com/*"
  ],
  "optional_permissions": [],

  "web_accessible_resources": [
    "icons/*",
    "google_icons/*",
    "background.jpg",
    "cached_wallpapers/*"
  ]
}
